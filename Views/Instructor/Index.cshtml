
@{
}

@model IEnumerable<DyITELEC1C.Models.Instructor>

<h1>Instructor List</h1>
<style>
  table {
    border-collapse: collapse;
    width: 100%;
  }

  th, td {
    padding: 10px;
    border: 1px solid black;
  }
</style>

<table class="padding">
    <tr>
        <th>Name:</th>
        <th>Date Hired</th>
        <th>Email:</th>
        <th>Rank</th>
    </tr>

     @foreach (var Instructor in Model)
        {
    <tr>
        <td>@Instructor.InstructorName</td>
        <td>@Instructor.DateHired</td>
        <td>@Instructor.InstructorEmail</td>
        <td>@Instructor.Rank</td>
    </tr>
    }

</table>